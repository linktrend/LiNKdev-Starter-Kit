{
  "name": "@starter/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --project tsconfig.typecheck.json",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "test": "vitest run --coverage",
    "test:unit": "vitest run src/__tests__/routers --coverage",
    "test:integration": "vitest run src/__tests__/integration --coverage",
    "test:watch": "vitest watch",
    "test:integration:watch": "vitest watch src/__tests__/integration"
  },
  "dependencies": {
    "@starter/types": "workspace:*",
    "@supabase/supabase-js": "^2.88.0",
    "@trpc/server": "^10.45.2",
    "superjson": "^2.2.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "next": "14.2.35",
    "react": "^18.3.1"
  }
}
